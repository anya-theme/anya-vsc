  "semanticHighlighting": false,
  "semanticTokenColors": {
    "method": "#ffc190"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment"],
      "settings": {
        "foreground": "#797979"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator", "entity.name.function.operator"],
      "settings": {
        "foreground": "#aaaaaa"
      }
    },
    {
      "name": "Namespace",
      "scope": [
        "entity.name.scope-resolution",
        "punctuation.separator.scope-resolution"
      ],
      "settings": {
        "foreground": "#d0d0d0"
      }
    },
    {
      "name": "Class",
      "scope": ["entity.name.type.class"],
      "settings": {
        "foreground": "#ffc190"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "entity.name.function.python"],
      "settings": {
        "foreground": "#ffc190"
      }
    },
    {
      "name": "Method", // Not exactly sure if this is the exact scope
      "scope": ["entity.name.function.member"],
      "settings": {
        "foreground": "#cccccc"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable"],
      "settings": {
        "foreground": "#dedede"
      }
    },
    {
      "name": "Storage | Type",
      "scope": ["storage.type", "storage.modifier", "keyword.type"],
      "settings": {
        "foreground": "#aaaaaa"
      }
    },
    {
      "name": "Keyword | Control - if, else, ---",
      "scope": ["keyword.other", "keyword.control"],
      "settings": {
        "foreground": "#aaaaaa"
      }
    },
    {
      "name": "String",
      "scope": ["string"],
      "settings": {
        "foreground": "#75b9c7"
      }
    },
    {
      "name": "String Escape",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#a1c7ce"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#a1c7ce"
      }
    },
    {
      "name": "Preprocessor",
      "scope": ["entity.name.function.preprocessor"],
      "settings": {
        "foreground": "#6ab6af"
      }
    },
    {
      "name": "Enumerator",
      "scope": ["variable.other.enummember"],
      "settings": {
        "foreground": "#6ab6af"
      }
    },
    {
      "name": "Property Name",
      "scope": ["support.type.property-name"],
      "settings": {
        "foreground": "#aaaaaa"
      }
    },

    // LANGUAGE SPECIFIC

    {
      "name": "(HTML, ---) Tags",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#aaaaaa"
      }
    },
    {
      "name": "(HTML, ---) Component - <!DOCTYPE>",
      "scope": ["support.class.component"],
      "settings": {
        "foreground": "#ffc190"
      }
    },
    {
      "name": "(C++) Templated Function - endl, ---",
      "scope": ["entity.name.function.templated"],
      "settings": {
        "foreground": "#dedede"
      }
    },
    {
      "name": "(CMake) Function",
      "scope": ["keyword.cmake"],
      "settings": {
        "foreground": "#ffc190"
      }
    }
  ]
}
